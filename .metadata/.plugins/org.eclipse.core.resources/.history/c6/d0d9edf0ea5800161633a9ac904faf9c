package daytwo;

public class dayTwoAssignment {

	public static void main(String[] args) {
		// #1
		String helloWorld = new String("Hello World");
		// #2
		int date = 1492;
		// #3
		boolean dog = true;
		// #4
		String todaysDate = "August 2nd 2016";
		// #5
		Double burgerPrice = 5.45;
		// #6
		char letter = 'X';
		// #1a
		System.out.println(helloWorld);
		// #2a
		System.out.println("Columbus Sailed the world in " + date);
		// #3a
		System.out.println("It is " + dog + " that I have a dog at home.");
		// #4a
		System.out.println("Today is " + todaysDate);
		// #5a
		System.out.println("I spent $" + burgerPrice + " on a cheeseburger");
		// #6a
		System.out.println(letter + " marks the spot");

		// Start of Array Assignment
		String[] name = new String[10];
		name[0] = "George";
		name[1] = "Chris";
		name[2] = "Stewart";
		name[3] = "Vafa";
		name[4] = "Ami";
		name[5] = "Mike";
		name[6] = "Kyle";
		name[7] = "John";
		name[8] = "Justin";
		name[9] = "Jamal";
		// Print Array 1D
		int count = 0;
		do {
			System.out.println("My name is " + name[count]);
			count++;
		} while (count < 10);
		
	}

}
	public static void main(String[] args) {
		
		String[] name = new String[10];
		name[0][0] = "George";
		name[0][1] = "Chris";
		name[0][2] = "Stewart";
		name[0][3] = "Vafa";
		name[0][4] = "Ami";
		name[1][0] = "Mike";
		name[1][1] = "Kyle";
		name[1][2] = "John";
		name[1][3] = "Justin";
		name[1][4] = "Jamal";
		
		//Print 2D Array
		int count = 0;
		int count2 = 0;
		if count2 < 3{
			do {
				System.out.println(name[count2][count]);
				count++;
			} while (count < 5);
		count2++;
		}
		
	
}
	
}
